<?xml version="1.0" encoding="UTF-8" ?>
<configuration>

  <appender name="Console" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} | %5p | %-200m | %C.%M\(%F:%L\)%n</pattern>
    </encoder>
  </appender>

 
  <logger name="com.javaetmoi.javabean" level="DEBUG"/>


  <root level="INFO">
    <appender-ref ref="Console" />
  </root>
</configuration>
